{
  "name": "chillmind-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-no-lint": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:dashboard": "playwright test tests/dashboard-*.spec.ts",
    "test:onboarding": "playwright test tests/onboarding-*.spec.ts",
    "test:auth": "playwright test tests/auth.spec.ts",
    "test:api": "playwright test tests/api-integration.spec.ts",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:ci": "playwright test --reporter=github",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test"
  },
  "dependencies": {
    "@google/genai": "^0.15.0",
    "@heroicons/react": "^2.2.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/leaflet": "^1.9.18",
    "chart.js": "^4.4.1",
    "firebase": "^10.7.0",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^11.0.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.511.0",
    "next": "15.3.1",
    "node-nlp": "^4.27.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.50.1",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^9.1.0",
    "recharts": "^2.12.0",
    "sonner": "^2.0.3",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  }
